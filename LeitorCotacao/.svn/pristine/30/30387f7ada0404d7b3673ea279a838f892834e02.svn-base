Function CalcImposto(n_Aliquota As Currency, n_BASE As Currency, n_CodEnt As Integer, c_Campo As String, Optional c_Tipo As String) As Variant
   cAux = LerNome("CGH", "CGH_COD = " & Val(n_CodEnt), c_Campo)
   CalcImposto = "0.00"
